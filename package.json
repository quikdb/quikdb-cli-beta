{
  "name": "quikdb-cli-beta",
  "version": "2.3.5",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quikdb/quikdb-cli-beta.git"
  },
  "author": "samsonajulor <samsonajulor@gmail.com>",
  "license": "MIT",
  "bin": {
    "quikdb": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node index.ts -u $1 -e $2",
    "watch": "tsc -w",
    "lnk": "yarn unlink && yarn link",
    "test": "npm run test:unit",
    "test:single": "yarn node --experimental-vm-modules $(yarn bin jest) $1",
    "test:unit": "yarn node --experimental-vm-modules $(yarn bin jest) --testPathPattern=unit",
    "test:integ": "yarn node --experimental-vm-modules $(yarn bin jest) --testPathPattern=integ",
    "push": "bash ./scripts/git_push.sh",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "coverage": "yarn node --experimental-vm-modules $(yarn bin jest) --coverage jest --testPathPattern=unit",
    "start": "node dist/index.js",
    "pub:p": "npm run push && npm run build && npm version patch && npm run push && git push --tags && npm publish --access public",
    "pub:mi": "npm run push && npm run build && npm version minor && npm run push && git push --tags && npm publish --access public",
    "pub:mj": "npm run push && npm run build && npm version major && npm run push && git push --tags && npm publish --access public"
  },
  "lint-staged": {
    "*.{js,ts,json,md,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@dfinity/agent": "^2.1.3",
    "@types/node": "^22.9.0",
    "@types/readline-sync": "^1.4.8",
    "axios": "^1.7.7",
    "commander": "^12.1.0",
    "email-validator": "^2.0.4",
    "quikdb-cli-beta": "^2.3.3",
    "readline-sync": "^1.4.10",
    "shelljs": "^0.8.5",
    "simple-git": "^3.27.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/shelljs": "^0.8.15",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "prettier": "^3.3.3"
  }
}
